<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width"/>
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    <style href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css"></style>
</head>
<body ng-app="app">
<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <div style="padding: 20px; font-size: 20px;">
                BLAC Dashboard
</div>
        </div>
        <div class="navbar-collapse collapse">
            @*<ul class="nav navbar-nav">
                <li>@Html.ActionLink("Home", "Index", "Home", new {area = ""}, null)</li>
                <li>@Html.ActionLink("API", "Index", "Help", new {area = ""}, null)</li>
            </ul>*@
        </div>
        <div class="userinfo pull-right">
            <div class="row">
                <div class="col-md-4"></div>
                <div class="col-md-8"></div>
            </div>
        </div>
    </div>
</div>
<div>
    @RenderBody()
</div>

@Scripts.Render("~/scripts/global")
@Scripts.Render("~/scripts/app")
<script type="text/javascript" src="@Url.Action("Constants", "Scripts")"></script>
@Scripts.Render("~/scripts/modules")
<script src="//cdnjs.cloudflare.com/ajax/libs/lodash.js/2.4.1/lodash.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/angular-google-maps/2.0.11/angular-google-maps.js"></script>
<script src="//maps.googleapis.com/maps/api/js?sensor=false"></script>
@RenderSection("scripts", false)
</body>
</html>